datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model users {
  id           Int     @id @default(autoincrement())
  UID          String  @unique @db.VarChar(255)
  name         String  @db.VarChar(255)
  lastName     String? @db.VarChar(255)
  email        String? @db.VarChar(255)
  passwordHash String  @unique @db.VarChar(255)
}

